    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "jest": "^24.3.0",
    "jest-cli": "^24.7.1",
    "laravel-mix": "^4.0.7",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.8.4",
    "resolve-url-loader": "^2.3.1"
